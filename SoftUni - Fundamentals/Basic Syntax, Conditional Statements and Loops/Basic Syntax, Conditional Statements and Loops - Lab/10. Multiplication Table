using System;

namespace _10.Multiplication_Table
{
    class Program
    {
        static void Main(string[] args)
        {
            int num = int.Parse(Console.ReadLine());
            int k = 0;
           while(true)
            {
                if(k==10)
                {
                    break;
                }
                k++;
                Console.WriteLine($"{num} X {k} = {num * k}");
            }
        }
    }
}
