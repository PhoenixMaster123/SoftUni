using System;

namespace Division
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            int devider = -1;
            
            if (n % 10 == 0)
            {
                devider = 10;             
            }
            else if (n % 7 == 0)
            {

                devider = 7;
            }
            else if (n % 6 == 0)
            {
                devider = 6;

            }
            else if (n % 2 == 0)
            {
                devider = 2;

            }
            else if (n % 3 == 0)
            {
                devider = 3;                
            }

            if(devider == -1)
            {
                Console.WriteLine("Not divisible");
            }
            else
            {
                Console.WriteLine($"The number is divisible by {devider}");
            }


        }
    }
}
