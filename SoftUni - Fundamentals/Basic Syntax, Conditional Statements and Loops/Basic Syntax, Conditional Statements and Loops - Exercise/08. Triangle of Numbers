using System;

namespace _08._Triangle_of_Numbers
{
    internal class Program
    {
        static void Main(string[] args)
        {
           int n = int.Parse(Console.ReadLine());
            for(int row = 1; row <= n; row++)
            {
                Console.WriteLine();
                for (int i = 1; i <= row; i++)
                {
                    Console.Write($"{ row} ");
                    
                }
            }
        }
    }
}
