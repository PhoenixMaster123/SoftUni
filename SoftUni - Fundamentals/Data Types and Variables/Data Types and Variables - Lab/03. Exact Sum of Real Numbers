using System;

namespace _03._Exact_Sum_of_Real_Numbers
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            decimal result = 0;
            for(int i = 1; i <= n; i++)
            {
                decimal num = decimal.Parse(Console.ReadLine());
                result += num;
            }
           Console.WriteLine(result);
        }
    }
}
